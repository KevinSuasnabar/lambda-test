{
  "name": "sab-functions",
  "author": "Squad CA",
  "version": "1.0.0",
  "description": "Sample OpenWhisk NodeJS serverless framework service.",
  "scripts": {
    "serverless": "sls offline --config=serverless.yml",
    "build": "npm i && node scripts/esbuild.config.mjs",
    "lambda:zip": "npm run build && npm i --prod && node scripts/aws.lambda.zip.mjs",
    "start": "npm run build && npm run serverless",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "files:verify": "npx tsc -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:clear": "jest --clearCache",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.462.0",
    "@aws-sdk/client-secrets-manager": "^3.462.0",
    "@aws-sdk/lib-dynamodb": "^3.462.0",
    "@babel/traverse": "^7.23.5",
    "@middy/core": "^5.0.3",
    "@middy/http-json-body-parser": "^5.0.3",
    "@types/ssh2-sftp-client": "^9.0.3",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "module-alias": "^2.2.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "ssh2-sftp-client": "^9.1.0",
    "ts-custom-error": "^3.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/adm-zip": "^0.5.5",
    "@types/aws-lambda": "^8.10.130",
    "@types/crypto-js": "^4.2.1",
    "@types/jest": "^29.5.10",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.10.1",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-mock-console": "^2.0.0",
    "jest-mock-extended": "^3.0.5",
    "json-schema-to-ts": "^2.9.2",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.49.0",
    "serverless-offline": "^13.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "file-type": "^18.7.0"
  },
  "jest": {
    "setupFiles": [
      "./jest.setup.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "dist/",
      "node_modules/",
      "coverage/"
    ],
    "coveragePathIgnorePatterns": [
      ".*\\.list\\.ts$",
      ".*\\.types\\.ts$",
      ".*\\.config\\.ts$",
      ".*\\.algorithms\\.ts$",
      "node_modules/",
      "coverage/",
      "dist/",
      ".eslintrc.js",
      "scripts/",
      "volume/"
    ],
    "moduleNameMapper": {
      "^@libs(|/.*)$": "<rootDir>/src/libs/$1",
      "^@functions(|/.*)$": "<rootDir>/src/functions/$1"
    }
  }
}
